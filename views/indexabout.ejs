<script>
    gsap.registerPlugin(ScrollTrigger);

    // Hero section animation
    gsap.from(".hero-content", {
        opacity: 0,
        y: 50,
        duration: 1,
        scrollTrigger: {
            trigger: ".hero-content",
            start: "top 80%",
            end: "bottom 20%",
            toggleActions: "play none none reverse"
        }
    });

    // Feedback cards animation
    gsap.utils.toArray(".feedback-card").forEach((card, i) => {
        gsap.from(card, {
            opacity: 0,
            y: 50,
            duration: 0.8,
            delay: i * 0.2,
            scrollTrigger: {
                trigger: card,
                start: "top 80%",
                end: "bottom 20%",
                toggleActions: "play none none reverse"
            }
        });
    });

    // About section animation
    gsap.from(".about-image", {
        opacity: 0,
        x: -50,
        duration: 1,
        scrollTrigger: {
            trigger: ".about-image",
            start: "top 80%",
            end: "bottom 20%",
            toggleActions: "play none none reverse"
        }
    });

    gsap.from(".about-content", {
        opacity: 0,
        x: 50,
        duration: 1,
        scrollTrigger: {
            trigger: ".about-content",
            start: "top 80%",
            end: "bottom 20%",
            toggleActions: "play none none reverse"
        }
    });
</script>

