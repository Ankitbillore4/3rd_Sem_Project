          <div class="main min-h-screen w-screen">
            <!-- Hero Section with Contact Information -->
            <section class="relative h-screen">
                <div class="absolute inset-0">
                    <img class="h-full w-full object-cover" src="https://img.freepik.com/free-photo/green-sofa-white-living-room-with-free-space_43614-834.jpg" alt="Contact background">
                    <div class="absolute inset-0 bg-black bg-opacity-50"></div>
                </div>
                <div class="relative z-10 h-full flex flex-col items-center justify-center px-4">
                    <div class="bg-white bg-opacity-90 rounded-3xl shadow-2xl p-8 w-full max-w-2xl">
                        <div class="flex flex-col sm:flex-row items-center justify-center mb-8">
                            <img class="h-24 sm:h-32 mb-4 sm:mb-0" src="/images/homeLogo.png" alt="Room Finder Logo">
                            <h1 class="text-4xl sm:text-5xl font-bold text-black ml-4">Room Finder</h1>
                        </div>
                        <h2 class="text-3xl font-semibold text-center mb-8">Contact Details</h2>
                        <div class="space-y-6">
                            <div class="flex items-center justify-center gap-4">
                                <i class="ri-mail-fill text-4xl text-black"></i>
                                <p class="text-xl sm:text-2xl">roomfinder@gmail.com</p>
                            </div>
                            <div class="flex items-center justify-center gap-4">
                                <i class="ri-phone-fill text-4xl text-black"></i>
                                <p class="text-xl sm:text-2xl">93-4343-2490</p>
                            </div>
                        </div>
                        <div class="flex justify-center gap-8 text-4xl mt-8">
                            <a href="#" class="text-black hover:text-gray-700 transition-colors duration-300">
                                <i class="ri-instagram-fill"></i>
                            </a>
                            <a href="#" class="text-black hover:text-gray-700 transition-colors duration-300">
                                <i class="ri-facebook-box-fill"></i>
                            </a>
                            <a href="#" class="text-black hover:text-gray-700 transition-colors duration-300">
                                <i class="ri-twitter-fill"></i>
                            </a>
                        </div>
                    </div>
                </div>
            </section>
    
            <!-- Contact Form Section -->
            <section class="py-16 md:py-24 bg-white">
                <div class="container mx-auto px-4">
                    <h2 class="text-3xl md:text-4xl font-bold text-center mb-12">Get in Touch</h2>
                    <form class="max-w-2xl mx-auto">
                        <div class="mb-6">
                            <label for="name" class="block text-gray-700 font-semibold mb-2">Name</label>
                            <input type="text" id="name" name="name" class="w-full px-4 py-2 border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-black" required>
                        </div>
                        <div class="mb-6">
                            <label for="email" class="block text-gray-700 font-semibold mb-2">Email</label>
                            <input type="email" id="email" name="email" class="w-full px-4 py-2 border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-black" required>
                        </div>
                        <div class="mb-6">
                            <label for="message" class="block text-gray-700 font-semibold mb-2">Message</label>
                            <textarea id="message" name="message" rows="4" class="w-full px-4 py-2 border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-black" required></textarea>
                        </div>
                        <div class="text-center">
                            <button type="submit" class="bg-black text-white font-semibold px-6 py-3 rounded-full hover:bg-gray-800 transition-colors duration-300">Send Message</button>
                        </div>
                    </form>
                </div>
            </section>
    
            <!-- Map Section -->
            <section class="py-16 md:py-24 bg-gray-100">
                <div class="container mx-auto px-4">
                    <h2 class="text-3xl md:text-4xl font-bold text-center mb-12">Find Us</h2>
                    <div class="aspect-w-16 aspect-h-9">
                        <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3022.2176767606308!2d-73.98823492342224!3d40.748440471440665!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x89c259a9b3117469%3A0xd134e199a405a163!2sEmpire%20State%20Building!5e0!3m2!1sen!2sus!4v1708653988075!5m2!1sen!2sus" width="100%" height="450" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
                    </div>
                </div>
            </section>
        </div>
    
        <script>
            gsap.registerPlugin(ScrollTrigger);
    
            // Hero section animation
            gsap.from(".bg-white", {
                opacity: 0,
                y: 50,
                duration: 1,
                scrollTrigger: {
                    trigger: ".bg-white",
                    start: "top 80%",
                    end: "bottom 20%",
                    toggleActions: "play none none reverse"
                }
            });
    
            // Contact form animation
            gsap.from("form", {
                opacity: 0,
                y: 50,
                duration: 1,
                scrollTrigger: {
                    trigger: "form",
                    start: "top 80%",
                    end: "bottom 20%",
                    toggleActions: "play none none reverse"
                }
            });
    
            // Map animation
            gsap.from(".aspect-w-16", {
                opacity: 0,
                y: 50,
                duration: 1,
                scrollTrigger: {
                    trigger: ".aspect-w-16",
                    start: "top 80%",
                    end: "bottom 20%",
                    toggleActions: "play none none reverse"
                }
            });
        </script>
        

        <!-- Footer -->
        <footer class="bg-gray-900 text-white py-12 ">
            <div class="container mx-auto px-4">
                <div class="grid grid-cols-1 md:grid-cols-3 gap-8">
