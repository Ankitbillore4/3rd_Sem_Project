               </div>
           </div>
           <!-- Desktop Navbar -->
           <div class="navbar hidden lg:flex h-[100px] w-full bg-slate-100 items-center justify-between pr-10 drop-shadow-xl fixed z-[1200]">
               <div class="navpart1 flex items-center">
                   <img class="h-[100px]" src="/images/homeLogo.png" alt="">
                   <div class="links text-xl pl-10 flex gap-12">
                       <a href="./" class="hover:border-b-2 hover:border-black">Home</a>
                       <a href="./about" class="hover:border-b-2 hover:border-black">About</a>
                       <a href="./contact" class="hover:border-b-2 hover:border-black">Contact</a>
                       <a href="./details" class="hover:border-b-2 hover:border-black">Details</a>
                       <a href="./login" class="hover:text-black ">Login</a>
                       <a href="./signup" class="hover:text-black">Sign Up</a>
